PROGRAM myTravelerClock;
CONST 
//<Constant declarations> //locs=29
  TEXTOUT = ' hi time traveler ';

{TYPE 
<Type declarations>} 

VAR 
//<Variable declarations>
  i, N, SN: integer;

//<FUNCTION>
//<PROCEDURE> 

BEGIN  //Main
//<Executable statements>
  N:= 4; SN:= 1000;
  for i:= 1 to round(Power(2,N)) do begin 
    Writeln(intToStr(i)+TEXTOUT + DateTimeToStr(AddHours(Now,-i)));
    Delay(SN);   //speed of time machine
  end;  
//<Definitions>  
   Writeln('go back to year 99 '+TEXTOUT+DateTimeToStr(AddHours(Now,-(round(Power(2,24))))));
   Writeln('years: '+intToStr(Round(Power(2,24)/24/365)));
   Writeln('years: '+intToStr(Round(Power(2,24)/(24*365))));
   Writeln(intToStr(i)+TEXTOUT + DateTimeToStr(Now-700000));
   Writeln(InttoRoman(2013));
END. 

